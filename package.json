{
  "name": "weather-focast-frontend",
  "version": "1.0.0",
  "description": "Node.js frontend proxy server for Flask backend.",
  "main": "Frontend/server.js",
  "scripts": {
    "start": "node Frontend/server.js",
    "dev": "npx concurrently --kill-others-on-fail \"npm run flask\" \"npm run node\"",
    "node": "nodemon Frontend/server.js",
    "flask": "python3 Backend/app.py"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "http-proxy-middleware": "^3.0.5",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.10",
    "tailwindcss": "^4.1.14"
  }
}
